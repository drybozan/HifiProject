@model List<HiFi.Dto.Dtos.CompPictureDto>

<div id="carouselExampleControls2" class="carousel slide mx-auto my-auto" data-bs-ride="carousel">
    <div class="carousel-inner">
        @foreach (var item in Model)
        {
            if (item.CompPicture != null)
            {
                <div class="carousel-item active">
                    <a>
                        <img src="http://localhost:51075/Uploads/CompPictures/@item.CompPicture" class="d-block w-100" height="100" width="200">
                    </a>
                </div>
            }
        }
    </div>
    <button class="carousel-control-prev dark-version" type="button" data-bs-target="#carouselExampleControls2" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next dark-version" type="button" data-bs-target="#carouselExampleControls2" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>